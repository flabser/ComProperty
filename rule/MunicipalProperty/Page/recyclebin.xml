    <caption name="view" source="KEYWORD">Корзина</caption>
    <caption name="datereg" source="KEYWORD">Дата регистрации</caption>
    <caption name="corr" source="KEYWORD">Отправитель</caption>
    <caption name="briefcontent" source="KEYWORD">Краткое содержание</caption>
    <caption name="btnRemove" source="KEYWORD">Очистить</caption>
    <caption name="btnRecover" source="KEYWORD">Восстановить</caption>
    <caption name="BTNDELETEDOCUMENT" source="KEYWORD">Удалить документ</caption>
    <caption name="name" source="KEYWORD">Название</caption>
