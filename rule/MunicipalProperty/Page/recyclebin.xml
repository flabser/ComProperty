<?xml version="1.0" encoding="utf-8"?>
<rule type="page" mode="on" history="on">
    <description></description>
    <caching>NO_CACHING</caching>
    <xsltfile>pages/page-recyclebin.xsl</xsltfile>
    <element mode="ON" type="INCLUDED_PAGE">
        <name>actionbar</name>
        <value>actionbar</value>
    </element>
