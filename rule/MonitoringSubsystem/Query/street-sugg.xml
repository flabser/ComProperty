<?xml version="1.0" encoding="utf-8"?>
<rule type="query" id="street-sugg">
	<description>Поиск улиц</description>
	<doctype>glossary</doctype>
	<query source="static">form = 'street' and viewtext ~ '$keyword'</query>
	<sortby source="static">VIEWTEXT1#asc</sortby>
	<cache mode="OFF"/>
	<field mode="on">	
		<name>viewtext</name>
		<value source="resultset">name</value>	
	</field>
	<field mode="on">	
		<name>code</name>
		<value source="resultset">code</value>	
	</field>
</rule>
