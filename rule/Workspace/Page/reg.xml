<?xml version="1.0" encoding="utf-8"?>
<rule type="page" mode="on" anonymous="on">
    <description>Workspace</description>
    <!-- <caching>CACHING_IN_USER_SESSION_SCOPE</caching> -->
    <caching>NO_CACHING</caching>
    <xsltfile>DEFAULT</xsltfile>
    <element mode="ON" type="SCRIPT">
        <events>
            <doscript source="JAVA_CLASS">workspace.page.form.RegForm</doscript>
        </events>
    </element>
    <caption name="logout"/>
    <caption name="another_comp"/>
    <caption name="user"/>
    <caption name="password"/>
    <caption name="login"/>
    <caption name="email"/>
    <caption name="sign_in"/>
    <caption name="sign_up"/>
    <caption name="fio"/>
    <caption name="org"/>
    <caption name="bin"/>
    <caption name="comment"/>
    <caption name="reg_request_accepted"/>
</rule>
