<?xml version="1.0" encoding="UTF-8"?>
<rule id="Workspace" mode="on" type="global">
    <description>Рабочая область</description>
    <orgname>АИС "Коммунальное имущество"</orgname>
    <logo>logo.png</logo>
    <appname>Рабочая область</appname>
    <database autodeploy="OFF">
        <name>ComProperty</name>
        <driver>org.postgresql.Driver</driver>
        <url>jdbc:postgresql://localhost:5433/PropertyObjects</url>
        <implementation>
            <idatabasedeployer>kz.lof.jpadatabase.DatabaseDeployer
            </idatabasedeployer>
            <idatabase>kz.lof.jpadatabase.Database</idatabase>
        </implementation>
        <username>postgres</username>
        <password>1</password>
    </database>
    <externalmodule type="STAFF">Structure</externalmodule>
    <defaultredirecturl>Provider?id=workspace</defaultredirecturl>
    <langs>
        <entry id="RUS">Русский</entry>
        <entry id="KAZ">Казакша</entry>
        <!-- <entry id="ENG">English</entry> <entry id="CHN">中国</entry> -->
    </langs>
     <skins>
        <entry default="on" id="classic" path="./webapps/Workspace/xsl">Classic</entry>
    </skins>
</rule>
