        <name>ComProperty</name>
        <driver>org.postgresql.Driver</driver>
        <url>jdbc:postgresql://localhost:5433/ComProperty</url>
	<username/>
		<password/><connectionid>kGcmwuH/jaNcH+ZvpEQwQA==@@@BvYd11KTEKFcH+ZvpEQwQA==</connectionid></database>
