<?xml version="1.0" encoding="UTF-8"?><rule id="Workspace" mode="on" type="global">
	<description>Рабочая область</description>
	<orgname>АИС "Коммунальное имущество"</orgname>
	<liccount>100</liccount>
	<logo>logo.png</logo>
	<appname>Рабочая область</appname>
	<database autodeploy="ON">
		<name>PropertyObjects</name>
		<driver>org.postgresql.Driver</driver>
		<url>jdbc:postgresql://localhost/PropertyObjects</url>
		<username/>
		<password/>
	<connectionid>pf6CJiCcT2K00oZyhcGJ7w==@@@WFDAk+Hkd08=</connectionid></database>
	<externalmodule type="STRUCTURE">Structure</externalmodule>
	<daemons>
		<cyclecontrol>
			<scheduler mode="off">
				<mininterval daysofweek="WORKWEEK,">360</mininterval>
			</scheduler>
		</cyclecontrol>
		<timewaiting>
			<scheduler mode="off">
				<mininterval daysofweek="WORKWEEK,">360</mininterval>
			</scheduler>
		</timewaiting>
		<recalculator>
			<scheduler mode="off">
				<mininterval daysofweek="WORKWEEK,">360</mininterval>
			</scheduler>
		</recalculator>
	</daemons>
	<entrypoint>Provider?type=static&amp;id=start</entrypoint>
	<langs>
		<entry id="RUS">Русский</entry>
		<entry id="KAZ">Казакша</entry>
		<!-- <entry id="ENG">English</entry>
		<entry id="CHN">中国</entry> -->		
	</langs>
	<skins>
		<entry default="on" id="classic" path="./webapps/Workspace/classic/xsl">Classic</entry>
	</skins>
	<markdelaysec>5</markdelaysec>
	<eds mode="OFF">
	</eds> 
</rule>