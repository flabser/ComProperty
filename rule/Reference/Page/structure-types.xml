<?xml version="1.0" encoding="utf-8"?>
<rule type="page" mode="on" history="on">
    <description>StructureType</description>
    <caching>NO_CACHING</caching>
    <xsltfile>*</xsltfile>
    <element mode="ON" type="SCRIPT">
        <name>view_content</name>
        <events>
            <doscript source="JAVA_CLASS">reference.page.view.StructureTypeView</doscript>
        </events>
    </element>
    <element mode="ON" type="INCLUDED_PAGE">
        <name></name>
        <value>outline</value>
    </element>
    <element mode="ON" type="INCLUDED_PAGE">
        <name>share_view_captions</name>
        <value>share_view_captions</value>
    </element>
</rule>
