<?xml version="1.0" encoding="utf-8"?>
<rule type="page" mode="on" history="on">
    <description>Country</description>
    <caching>NO_CACHING</caching>
    <xsltfile>*</xsltfile>
    <element mode="ON" type="SCRIPT">
        <events>
            <doscript source="JAVA_CLASS">reference.page.form.CountryForm</doscript>
        </events>
    </element>
    <element mode="ON" type="INCLUDED_PAGE">
        <name></name>
        <value>outline</value>
    </element>
    <element mode="ON" type="INCLUDED_PAGE">
        <name>share_view_captions</name>
        <value>share_view_captions</value>
    </element>
    <caption name="country" source="KEYWORD">Country</caption>
</rule>
