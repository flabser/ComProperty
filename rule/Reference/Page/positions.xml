<?xml version="1.0" encoding="utf-8"?>
<rule type="page" mode="on" history="on">
    <description>Positions</description>
    <caching>NO_CACHING</caching>
    <xsltfile>DEFAULT</xsltfile>
    <element mode="ON" type="SCRIPT">
        <name>view_content</name>
        <events>
            <doscript source="JAVA_CLASS">reference.page.view.PositionView</doscript>
        </events>
    </element>
    <element mode="ON" type="INCLUDED_PAGE">
        <value>outline</value>
    </element>
    <element mode="ON" type="INCLUDED_PAGE">
        <value>share_view_captions</value>
    </element>
    <caption name="positions" source="KEYWORD">Positions</caption>
</rule>
