<?xml version="1.0" encoding="windows-1251"?>
<rule type="query" id="street">
	<description></description>
	<doctype>glossary</doctype>
	<query source="static">form = 'street' and is_actual = 'true'</query>
	<sortby source="static">VIEWTEXT1#asc</sortby>
	<field mode="on">	
		<name>viewtext</name>
		<value source="resultset">name</value>	
	</field>
	<field mode="on">	
		<name>code</name>
		<value source="resultset">code</value>	
	</field>
</rule>