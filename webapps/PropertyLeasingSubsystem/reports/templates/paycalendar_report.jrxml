	<parameter name="HOST" class="java.lang.String"/>
	<queryString>
		<![CDATA[select * from maindocs
left join (select viewtext as objectname, docid, ddbid as objectddbid, form as objectform from maindocs) as objectname on
maindocs.parentdocid = objectname.docid
left join (select value as tarif, docid from custom_fields where name = 'tarif') as tarif on
maindocs.docid = tarif.docid
left join (select value as startcontractdate, docid from custom_fields where name = 'startcontractdate') as startdate on
maindocs.docid = startdate.docid
left join (select value as endrentdate, docid from custom_fields where name = 'endrentdate') as enddate on
maindocs.docid = enddate.docid
left join (select value as contractnum, docid from custom_fields where name = 'regnumber') as contractnum on
maindocs.docid = contractnum.docid
left join (select viewtext as leasingholdername, custom_fields.docid as docid, custom_fields.valueasnumber as balanceholder from maindocs, custom_fields where maindocs.docid = custom_fields.valueasnumber and custom_fields.name = 'leasingholder') as leasingholder on
maindocs.docid = leasingholder.docid
left join (select ddbid as pcddbid, parentdocid as pcparentdocid from maindocs where form = 'paycalendar') as paycalendar on
maindocs.docid = paycalendar.pcparentdocid
where form = 'orderleasing'
and maindocs.docid in (select parentdocid from maindocs where form = 'paycalendar')
$P!{BALANCEHOLDERCOND}
$P!{CONTRACTNUMCOND}]]>
	</queryString>
